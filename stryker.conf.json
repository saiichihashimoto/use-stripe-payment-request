{
  "checkers": ["typescript"],
  "packageManager": "npm",
  "mutate": ["src/**/*.ts", "!**/*.{d,spec}.ts"],
  "reporters": ["clear-text", "progress"],
  "testRunner": "jest",
  "tsconfigFile": "tsconfig.json",
  "jest": {
    "configFile": "jest.config.js"
  },
  "thresholds": {
    "high": 90,
    "low": 80,
    "break": 70
  }
}
